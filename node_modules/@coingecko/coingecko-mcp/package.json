{
  "name": "@coingecko/coingecko-mcp",
  "version": "1.7.3",
  "description": "The official MCP Server for the Coingecko API",
  "author": "Coingecko <eason.lim@coingecko.com>",
  "types": "./index.d.ts",
  "main": "./index.js",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coingecko/coingecko-typescript.git",
    "directory": "packages/mcp-server"
  },
  "homepage": "https://github.com/coingecko/coingecko-typescript/tree/main/packages/mcp-server#readme",
  "license": "Apache-2.0",
  "packageManager": "yarn@1.22.22",
  "private": false,
  "scripts": {
    "test": "jest",
    "build": "bash ./build",
    "format": "prettier --write --cache --cache-strategy metadata . !dist",
    "tsn": "ts-node -r tsconfig-paths/register",
    "lint": "eslint --ext ts,js .",
    "fix": "eslint --fix --ext ts,js ."
  },
  "dependencies": {
    "@coingecko/coingecko-typescript": "^1.7.3",
    "@modelcontextprotocol/sdk": "^1.11.5",
    "yargs": "^17.7.2",
    "@cloudflare/cabidela": "^0.2.4",
    "zod": "^3.25.20",
    "zod-to-json-schema": "^3.24.5"
  },
  "bin": {
    "mcp-server": "./index.js"
  },
  "imports": {
    "@coingecko/coingecko-mcp": ".",
    "@coingecko/coingecko-mcp/*": "./src/*"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "types": "./index.d.mts",
      "default": "./index.mjs"
    },
    "./compat": {
      "import": "./compat.mjs",
      "require": "./compat.js"
    },
    "./compat.js": {
      "default": "./compat.js"
    },
    "./compat.mjs": {
      "default": "./compat.mjs"
    },
    "./dynamic-tools": {
      "import": "./dynamic-tools.mjs",
      "require": "./dynamic-tools.js"
    },
    "./dynamic-tools.js": {
      "default": "./dynamic-tools.js"
    },
    "./dynamic-tools.mjs": {
      "default": "./dynamic-tools.mjs"
    },
    "./index": {
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./index.js": {
      "default": "./index.js"
    },
    "./index.mjs": {
      "default": "./index.mjs"
    },
    "./options": {
      "import": "./options.mjs",
      "require": "./options.js"
    },
    "./options.js": {
      "default": "./options.js"
    },
    "./options.mjs": {
      "default": "./options.mjs"
    },
    "./server": {
      "import": "./server.mjs",
      "require": "./server.js"
    },
    "./server.js": {
      "default": "./server.js"
    },
    "./server.mjs": {
      "default": "./server.mjs"
    },
    "./tools/*.mjs": {
      "default": "./tools/*.mjs"
    },
    "./tools/*.js": {
      "default": "./tools/*.js"
    },
    "./tools/*": {
      "import": "./tools/*.mjs",
      "require": "./tools/*.js"
    },
    "./tools": {
      "import": "./tools.mjs",
      "require": "./tools.js"
    },
    "./tools.js": {
      "default": "./tools.js"
    },
    "./tools.mjs": {
      "default": "./tools.mjs"
    }
  }
}